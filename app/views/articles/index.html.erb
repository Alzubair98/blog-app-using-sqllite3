<h1 class=" text-center mt-4">All Articles</h1>


<%= render 'article'%>

<%if logged_in?%>
<div class="button-cont">
  <%= link_to "Create New Article", new_article_path, class: "btn btn-outline-dark" %>
</div>
<%end%>

<div class="apple_pagination mb-4">
  <%= will_paginate @articles, :container => false %>
</div>